{
  "name": "workshop",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-react-native": "^3.2.1",
    "eslint-plugin-standard": "^3.1.0",
    "flow-bin": "^0.74.0",
    "flow-coverage-report": "^0.5.0",
    "husky": "^0.14.3",
    "jest-expo": "~27.0.0",
    "npm-run-all": "^4.1.3",
    "pretty-quick": "^1.6.0",
    "react-dom": "^16.4.1",
    "react-native-material-design": "^0.3.7",
    "react-native-scripts": "1.14.0",
    "react-native-vector-icons": "^4.6.0",
    "react-test-renderer": "16.3.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "jest": "jest --config jest.config.js",
    "test": "npm run jest -- --coverage",
    "test:watch": "npm run jest -- --watch",
    "flow-coverage": "flow-coverage-report -i 'src/**/*.{js,jsx}' -x 'src/test/**' -t html -t json -t text --threshold 80",
    "format": "prettier --write *.{js,jsx}",
    "precommit": "pretty-quick --staged",
    "check-health": "npm-run-all -s test format flow-coverage",
    "lint": "eslint -c .eslintrc.config.js src/*",
    "lint:fix": "npm run lint -- --fix"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "@types/react": "^16.4.0",
    "@types/react-native": "^0.55.22",
    "eslint": "^4.19.1",
    "expo": "^27.0.1",
    "flow": "^0.2.3",
    "jest": "^23.1.0",
    "native-base": "^2.6.0",
    "prettier": "^1.13.5",
    "react": "16.3.1",
    "react-native": "^0.55.4",
    "react-navigation": "^2.5.1",
    "redux": "^4.0.0"
  }
}
